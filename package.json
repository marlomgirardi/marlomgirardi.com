{
  "name": "gatsby-starter-blog-amp-to-pwa",
  "private": true,
  "description": "A starter for a blog(AMP and PWA) powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "Tomoyuki Kashiro <ksrtmyk@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "debug": "node --nolazy --inspect-brk node_modules/gatsby/dist/bin/gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "amp": "find public/amp -type f | xargs amphtml-validator",
    "test": "npm run amp"
  },
  "bugs": {
    "url": "https://github.com/tomoyukikashiro/gatsby-starter-blog-amp-to-pwa/issues"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "gatsby": "^2.19.23",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-feed-mdx": "^1.0.1",
    "gatsby-plugin-google-analytics": "^2.1.36",
    "gatsby-plugin-html2amp": "^1.3.1",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-mdx": "^1.0.74",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-typography": "^2.3.22",
    "gatsby-remark-copy-linked-files": "^2.1.37",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-prismjs": "^3.3.31",
    "gatsby-remark-responsive-iframe": "^2.2.32",
    "gatsby-remark-smartypants": "^2.1.21",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-sharp": "^2.3.16",
    "prismjs": "^1.19.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "amphtml-validator": "^1.0.30",
    "prettier": "^1.19.1"
  },
  "homepage": "https://github.com/tomoyukikashiro/gatsby-starter-blog-amp-to-pwa#readme",
  "keywords": [
    "gatsby",
    "amp",
    "pwa"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomoyukikashiro/gatsby-starter-blog-amp-to-pwa.git"
  }
}
